<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>basicServlet</servlet-name>
    <servlet-class>ServletFirst.FirstServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>basicServlet</servlet-name>
    <url-pattern>/get/basic</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>customGenericServlet</servlet-name>
    <servlet-class>ServletFirst.SecondGenericServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>customGenericServlet</servlet-name>
    <url-pattern>/get/generic</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>httpservlet</servlet-name>
    <servlet-class>ServletFirst.CustomHttpServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>httpservlet</servlet-name>
    <url-pattern>/get/http</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>registration.RegistrationHandler</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>


</web-app>
